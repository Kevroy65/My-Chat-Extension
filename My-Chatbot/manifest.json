{
  "manifest_version": 3,
  "name": "My-Chatbot",
  "version": "1.0",
  "icons": {
    "128": "/images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png"
    }
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://my-chatbot-d59f5.web.app/*"],
      "js": ["popup.js"]
    }
  ]
}
